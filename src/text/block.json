{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "s8/text",
	"version": "0.1.0",
	"title": "Text",
	"category": "text",
	"icon": "edit-large",
	"description": "Characters, words or even a sentence or two.",
	"supports": {
		"html": false,
		"__experimentalSlashInserter": true,
		"typography": {
			"fontSize": true
		}
	},
	"attributes": {
		"content": {
			"type": "string",
			"source": "html",
			"selector": "span",
			"default": "",
			"__experimentalRole": "content"
		},
		"placeholder": {
			"type": "string"
		}
	},
	"textdomain": "s8-lineup",
	"editorScript": "file:../text.js",
	"editorStyle": "file:../text.css"
}
